{
  "name": "tutWebpack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node config/dev-server.js",
    "build:server": "node_modules/.bin/http-server dist/",
    "build": "node config/build.js",
    "remap": "./node_modules/.bin/remap-istanbul --input ./coverage/coverage-final.json --output ./coverage/remapped --type html",
    "test": "node_modules/.bin/karma start; npm run remap",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "doc": "node_modules/.bin/typedoc ./app/ --exclude app/**/*.spec.ts --ignoreCompilerErrors --excludeExternals --experimentalDecorators --target ES5 --out ./doc",
    "mock_data": "node mock/generate_members.js"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx)$",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "dependencies": {
    "@types/node": "^7.0.0",
    "@types/react": "^15.0.6",
    "@types/react-dom": "^0.14.22",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^3.0.0",
    "@types/react-router-redux": "^4.0.39",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-polyfill": "^6.22.0",
    "bootstrap-sass": "^3.3.7",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "@types/react-addons-test-utils": "^0.14.17",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "compression-webpack-plugin": "^0.3.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^18.1.0",
    "node-sass": "^4.3.0",
    "opn": "^4.0.2",
    "ora": "^1.1.0",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^4.1.1",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "typescript-babel-jest": "^1.0.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  }
}
